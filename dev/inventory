[#apim]
#apim_1 ansible_host=[ip_address] ansible_user=[ssh_user]

# Example
# [apim]
# apim_1 ansible_host=172.28.128.4 ansible_user=vagrant

# Example
# [micro-integrator]
# micro-integrator_1 ansible_host=172.28.128.4 ansible_user=vagrant

[apim]
apim-tm_1              ansible_host=tm.apim.com    ansible_user=azureuser   ansible_ssh_private_key_file=files/keys/apimcp_key.pem
apim-gateway_1         ansible_host=gw.apim.com    ansible_user=azureuser   ansible_ssh_private_key_file=files/keys/apimgw_key.pem
apim-control-plane_1   ansible_host=cp.apim.com  ansible_user=azureuser   ansible_ssh_private_key_file=files/keys/apimcp_key.pem

